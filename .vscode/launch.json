    "version": "0.2.0",
    "configurations": [
        {
            "name": "Integrate",
            "type": "python",
            "request": "launch",
            "module": "ypackage.integrate",
            "args": [
                "../IstanbulUniversity-CE",
                "-rg",
                "-ll",
                "2",
                "-s"
            ]
        },
        {
            "name": "Integrat More",
            "type": "python",
            "request": "launch",
            "module": "ypackage.integrate",
            "args": [
                "../IstanbulUniversity-CE",
                "../YWiki",
            ]
        },
        {
            "name": "Test Store",
            "type": "python",
            "request": "launch",
            "module": "ypackage.integrate",
            "args": [
                "../YWelcome",
                "-us"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "GDrive - Base",
